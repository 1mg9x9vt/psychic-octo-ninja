---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <!-- stolen from coyled.dcom -->
 
  <title>Psychic Octo Ninja</title>
  <link href="http://coyled.com/"/>
  <link type="application/atom+xml" rel="self" href="http://hptt.github.com/psychic-octo-ninja/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://hptt.github.com/psychic-octo-ninja/</id>
  <author>
    <name>Henrik P.</name>
  </author>

  {% for post in site.posts %}
  <entry>
          <id>http://hptt.github.com/psychic-octo-ninja/{{ post.id }}</id>
          <link type="text/html" rel="alternate" href="http://hptt.github.com/psychic-octo-ninja/{{ post.url }}"/>
          <title>{{ post.title }}</title>
          <published>{{ post.date | date_to_xmlschema }}</published>
          <updated>{{ post.date | date_to_xmlschema }}</updated>
          <author>
                  <name>Henrik P.</name>
                  <uri>http://hptt.github.com/psychic-octo-ninja/</uri>
          </author>
          <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
