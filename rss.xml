---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <!-- stolen from coyled.dcom -->
  <channel>
    <title>Psychic Octo Ninja</title>
    <link>http://hptt.github.com/psychic-octo-ninja/</link>
    <atom:link href="http://hptt.github.com/psychic-octo-ninja/rss.xml" rel="self" type="application/rss+xml" />
    <description>experimenting with jekyll and poole</description>
    <!--language>en-us</language-->
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts %}
    <item>
      <title>{{ post.title }}</title>
      <link>http://hptt.github.com/psychic-octo-ninja/{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>github.com/hptt (Henrik P.)</author>
      <guid>http://hptt.github.com/psychic-octo-ninja/{{ post.id }}</guid>
      <description>{{ post.content | xml_escape }}</description>
    </item>
    {% endfor %}

  </channel> 
</rss>
